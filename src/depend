dtypes.h: generator.rb
	$(RUBY) -I$(srcdir) $(srcdir)/generator.rb

dtypes.c: generator.rb
	$(RUBY) -I$(srcdir) $(srcdir)/generator.rb

$(DLLIB): $(OBJS) Makefile dtypes.h dtypes.c
	$(ECHO) linking shared-object $(DLLIB)
	@-$(RM) $(@)
	$(Q) $(LDSHARED) -o $@ $(OBJS) $(LIBPATH) $(DLDFLAGS) $(LOCAL_LIBS) $(LIBS)
